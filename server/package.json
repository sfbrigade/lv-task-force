{
  "type": "module",
  "name": "full-stack-starter-server",
  "version": "6.0.0",
  "main": "app.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "dev": "prisma db push --accept-data-loss; fastify start -w -l info -P app.js",
    "lint": "eslint --fix",
    "pretest": "eslint",
    "prisma:generate": "prisma generate",
    "prisma:studio": "prisma studio",
    "start": "fastify start -l info app.js",
    "test": "node --test-concurrency 1 --test $(find test -name '*.test.js')"
  },
  "license": "AGPL-3.0-only",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.893.0",
    "@aws-sdk/cloudfront-signer": "^3.893.0",
    "@aws-sdk/s3-request-presigner": "^3.893.0",
    "@fastify/autoload": "^6.3.1",
    "@fastify/secure-session": "^8.2.0",
    "@fastify/static": "^8.2.0",
    "@fastify/swagger": "^9.5.1",
    "@fastify/url-data": "^6.0.3",
    "@prisma/client": "^6.16.2",
    "@scalar/fastify-api-reference": "^1.35.7",
    "accepts": "^1.3.8",
    "bcrypt": "^6.0.0",
    "dotenv": "^17.2.2",
    "ejs": "^3.1.10",
    "email-templates": "^12.0.3",
    "fastify": "^5.6.0",
    "fastify-plugin": "^5.0.1",
    "fastify-zod-openapi": "^5.4.0",
    "http-status-codes": "^2.3.0",
    "inflection": "^3.0.2",
    "lodash": "^4.17.21",
    "luxon": "^3.7.2",
    "mime-types": "^3.0.1",
    "nodemailer": "^7.0.6",
    "pm2": "^6.0.11",
    "zod": "^4.1.11",
    "zod-openapi": "^5.4.1"
  },
  "devDependencies": {
    "@getbigger-io/prisma-fixtures-cli": "^2.0.1",
    "@testcontainers/postgresql": "^11.6.0",
    "c8": "^10.1.3",
    "eslint": "^9.36.0",
    "fastify-cli": "^7.4.0",
    "neostandard": "^0.12.2",
    "nodemailer-mock": "^2.0.9",
    "prisma": "^6.16.2",
    "testcontainers": "^11.6.0",
    "yaml": "^2.8.1"
  },
  "imports": {
    "#lib/*.js": "./lib/*.js",
    "#models/*.js": "./models/*.js",
    "#prisma/*.js": "./prisma/*.js",
    "#test/*.js": "./test/*.js"
  }
}
